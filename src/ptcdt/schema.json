        "matchingRules": {
            "additionalProperties": false,
            "patternProperties": {
                "^\\$.*$": {
                    "oneOf": [
                        {
                            "additionalProperties": false,
                            "properties": {
                                "match": {
                                    "enum": ["type"],
                                    "type": "string"
                                },
                                "max": {
                                    "type": "number"
                                },
                                "min": {
                                    "type": "number"
