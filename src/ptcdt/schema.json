{
    "$schema": "http://json-schema.org/draft-04/schema",
    "definitions": {
        "headers": {
            "additionalProperties": {
                "type": "string"
            },
            "type": "object"
        },
        "interaction": {
            "additionalProperties": false,
            "properties": {
